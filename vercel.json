{
  "version": 2,
  "cleanUrls": true,
  "outputDirectory": ".",
  "crons": [
    {
      "path": "/api/cron_frankenstein",
      "schedule": "0 8 * * *"
    },
    {
      "path": "/api/watchdog?action=cron",
      "schedule": "0 * * * *"
    }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" }
  ]
}
